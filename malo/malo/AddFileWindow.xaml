<Window x:Class="malo.AddFileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:malo"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="MALO - Add File" Height="700" Width="500">
    <Grid>
        <StackPanel Margin="12">
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="36">
                Add File
            </TextBlock>

            <DockPanel>
                <Frame Name="frameAddFile" Margin="12"></Frame>
            </DockPanel>
        </StackPanel>
    </Grid>
</Window>
