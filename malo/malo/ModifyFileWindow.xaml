<Window x:Class="malo.ModifyFileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:malo"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="ModifyFileWindow" Height="700" Width="500">
    <Grid>
        <StackPanel>
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="36">
                Modify File
            </TextBlock>
            <DockPanel>
                <Frame Name="frameModifyFile" Margin="12"></Frame>
            </DockPanel>
            
        </StackPanel>
    </Grid>
</Window>
